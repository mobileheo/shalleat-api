{
  "name": "shalleat_api",
  "version": "1.0.0",
  "description": "Backend server to provide API for ShallEat front-end with React",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "dev": "nodemon --inspect=0.0.0.0:7778 app.js",
    "db:create": "createdb --echo shall_eat_api_dev",
    "db:drop": "dropdb --echo shall_eat_api_dev",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:make:seed": "knex seed:make",
    "db:make:migration": "knex migrate:make",
    "testdb:create": "createdb --echo shall_eat_api_test",
    "test": "nyc mocha --timeout 10000 test/**/*.spec.js --recursive --exit"
  },
  "keywords": [
    "Express.js",
    "Passport.js",
    "PostgreSQL",
    "Knex",
    "Objection.js"
  ],
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "connect-session-knex": "^1.4.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-promise-router": "^3.0.2",
    "express-session": "^1.15.6",
    "joi": "^13.4.0",
    "jsonwebtoken": "^8.2.2",
    "knex": "^0.14.6",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "objection": "^1.1.8",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-instagram": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "faker": "^4.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.4",
    "nyc": "^12.0.2"
  },
  "author": "Sunny (Yongseok)",
  "license": "ISC"
}
